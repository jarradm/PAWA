<div id="Inherit Box">
<label>Choose Destination Folder</label>
    <form id="folderSelectionForm">
        <select name="folderList" id="folderList">
            @{
                PAWA.Classes.GetFolderFormSelectOptions pwa = new PAWA.Classes.GetFolderFormSelectOptions(WebSecurity.CurrentUserId);
                @Html.Raw(pwa.returnHTML);
            }
        </select> 
        <button id="moveFolderSubmit" type="button" value="Go" onclick="moveSubmitButton()">Go</button>
    </form>
</div>
